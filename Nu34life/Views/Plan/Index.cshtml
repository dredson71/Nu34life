

@model IEnumerable<Nu34life.Models.Plan>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Index</h2>
<p>
    @Html.ActionLink("Crear Plan", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Description
        </th>

        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>


        </tr>
    }
</table>
@section scripts{
    <script>
$(".nElimina").click(function (event) {
            if (confirm('Estas seguro de eliminar?') == false){
                event.preventDefault();
            }
        });</script>
}